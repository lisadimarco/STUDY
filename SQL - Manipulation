CREATE TABLE books (
id INTEGER,
name TEXT,
author TEXT,
read DATE
);

INSERT INTO (name, author, read)
	VALUES (1, 'The_Metamorphosis', 'Kafka', '2023-08-12'), (2, 'Memories_Dreams_Reflections'. 'Jung', '2024-09-15);

ALTER TABLE books
ADD COLUMN rating INTEGER;

UPDATE friends
SET rating = 9 WHERE id = 1
SET rating = 8 WHERE id = 2;
